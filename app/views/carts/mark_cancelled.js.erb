/* Check if there are any errors */

if(<%= @cart.errors.any? %>){
	var key;
	var value;

	<% @cart.errors.each do |key, value| %>
		key = "<%= key %>";
		value = "<%= value %>";
		handleError('cart', key, value);
	<% end %>
}

/* If there aren't, render purchases */

else {
	// With turbolinks, you can do a lot of crazy things. For instance:
	Turbolinks.visit("<%= j purchases_path %>");
}